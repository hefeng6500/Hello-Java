package com.dadacar;

/*
 * 定义货车类
 * */
public class TruckCar {
	String name;
	int loadWeight;
	int rentDay;
	
	public TruckCar(String name, int loadWeight, int rentDay) {
		this.name = name;
		this.setWeight(loadWeight);
		this.rentDay = rentDay;
	}
	
	public void setWeight(int loadWeight) {
		this.loadWeight = loadWeight;
	}
	public int getWeight() {
		return loadWeight;
	}
}